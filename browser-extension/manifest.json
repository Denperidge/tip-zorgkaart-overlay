{
	"manifest_version": 2,
	"name": "tip-zorgkaart-goggles",
	"description": "Overlay the different TIP zorgkaart care providers with the positive & negative experiences of other trans people",
	"version": "0.0.1",
	"icons": {
		"64": "icons/icon.png"
	},
	"permissions": [
		"scripting"
	],
	"background": {
		"scripts": [
			"background_script.js"
		]
	},
	"content_scripts": [
		{
			"matches": [
				"http*://www.transgenderinfo.be/nl/zorgkaart/*"
			],
			"js": [
				"content_script.js"
			]
		}
	],
	"options_ui": {
		"page": "options/index.html"
	}
}